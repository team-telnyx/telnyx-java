    "description": "Returns the WDR Reports that match the given parameters.",
    "operationId": "GetWdrReports",
    "tags": [
      "Reporting"
    ],
    "parameters": [
      {
        "$ref": "../components/parameters/PageNumber.json"
      },
      {
        "$ref": "../components/parameters/PageSize.json"
      }
    ],
    "responses": {
      "200": {
        "$ref": "../components/responses/GetWdrReportsResponse.json"
      },
      "401": {

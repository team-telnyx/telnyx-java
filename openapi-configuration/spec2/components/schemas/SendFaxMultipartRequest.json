{
  "type": "object",
  "title": "Send fax multipart request",
  "required": [
    "connection_id",
    "contents",
    "from",
    "to"
  ],
  "properties": {
    "connection_id": {
      "$ref": "./ConnectionId.json"
    },
    "contents": {
      "description": "The file you want to upload. The maximum allowed size is 20 MB. contents and media_name/media_url can't be submitted together.",
      "type": "string",
      "format": "binary"
    },
    "to": {
      "$ref": "./To.json"
    },
    "from": {
      "$ref": "./From.json"
    },
    "quality": {
      "$ref": "./Quality.json"
    },
    "t38_enabled": {
      "$ref": "./T38Enabled.json"
    },
    "monochrome": {
      "$ref": "./Monochrome.json"
    },
    "store_media": {
      "$ref": "./StoreMedia.json"
    },
    "store_preview": {
      "$ref": "./StorePreview.json"
    }
  }
}
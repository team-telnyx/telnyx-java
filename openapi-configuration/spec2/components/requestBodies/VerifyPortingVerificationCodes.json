{
  "description": "A list of phone numbers and their verification codes",
  "required": true,
  "content": {
    "application/json": {
      "schema": {
        "type": "object",
        "properties": {
          "verification_codes": {
            "type": "array",
            "maxItems": 100,
            "items": {
              "type": "object",
              "properties": {
                "phone_number": {
                  "type": "string"
                },
                "code": {
                  "type": "string"
                }
              }
            }
          }
        },
        "example": {
          "verification_codes": [
            {
              "phone_number": "+61424000001",
              "code": "12345"
            },
            {
              "phone_number": "+61424000002",
              "code": "54321"
            }
          ]
        }
      }
    }
  }
}
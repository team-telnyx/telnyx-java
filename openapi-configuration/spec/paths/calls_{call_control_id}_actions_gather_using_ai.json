{
  "post": {
    "summary": "Gather using AI",
    "description": "Gather parameters defined in the request payload using a voice assistant.\n\n You can pass parameters described as a JSON Schema object and the voice assistant will attempt to gather these informations. \n\n**Expected Webhooks (see [callback schema](https://developers.telnyx.com/api/call-control/call-gather-using-ai#callbacks) below):**\n\n- `call.ai_gather.ended`\n- `call.ai_gather.partial_results` (if `send_partial_results` is set to `true`)\n- `call.ai_gather.tool_call` (if `assistant.tools` are configured)",
    "operationId": "callGatherUsingAI",
    "tags": [
      "Call Commands"
    ],
    "parameters": [
      {
        "$ref": "../components/parameters/CallControlId.json"
      }
    ],
    "requestBody": {
      "description": "Gather using AI request",
      "required": true,
      "content": {
        "application/json": {
          "schema": {
            "$ref": "../components/schemas/GatherUsingAIRequest.json"
          }
        }
      }
    },
    "responses": {
      "200": {
        "$ref": "../components/responses/CallControlCommandResponse.json"
      },
      "default": {
        "$ref": "../components/responses/GenericErrorResponse.json"
      }
    },
    "callbacks": {
      "CallAIGatherEnded": {
        "{$request.body#/webhook_url}": {
          "post": {
            "summary": "Calls gather using ai call ai gather ended",
            "requestBody": {
              "required": true,
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "../components/schemas/CallAIGatherEndedEvent.json"
                  }
                }
              }
            },
            "responses": {
              "200": {
                "description": "The server must return an HTTP 200, otherwise we will reattempt delivery at the failover webhook URL."
              }
            }
          }
        }
      },
      "CallAIGatherPartialResults": {
        "{$request.body#/webhook_url}": {
          "post": {
            "summary": "Calls gather using ai call ai gather partial results",
            "requestBody": {
              "required": true,
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "../components/schemas/CallAIGatherPartialResultsEvent.json"
                  }
                }
              }
            },
            "responses": {
              "200": {
                "description": "The server must return an HTTP 200, otherwise we will reattempt delivery at the failover webhook URL."
              }
            }
          }
        }
      },
      "CallAIGatherToolCall": {
        "{$request.body#/webhook_url}": {
          "post": {
            "summary": "Call gather using ai call ai gather tool call",
            "requestBody": {
              "required": true,
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "../components/schemas/CallAIGatherToolCallEvent.json"
                  }
                }
              }
            },
            "responses": {
              "200": {
                "description": "The server must return an HTTP 200. The body of the response will be used as the result of the tool call.",
                "content": {
                  "application/json": {
                    "schema": {
                      "type": "object",
                      "title": "Tool Call Response"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "x-latency-category": "interactive"
  }
}
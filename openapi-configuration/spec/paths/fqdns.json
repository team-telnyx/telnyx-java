{
  "get": {
    "summary": "List FQDNs",
    "description": "Get all FQDNs belonging to the user that match the given filters.",
    "operationId": "ListFqdns",
    "tags": [
      "FQDNs"
    ],
    "x-group-parameters": "true",
    "x-endpoint-cost": "medium",
    "parameters": [
      {
        "$ref": "../components/parameters/PageNumber.json"
      },
      {
        "$ref": "../components/parameters/PageSize.json"
      },
      {
        "name": "filter[connection_id]",
        "description": "ID of the FQDN connection to which the FQDN belongs.",
        "in": "query",
        "schema": {
          "type": "string"
        }
      },
      {
        "name": "filter[fqdn]",
        "description": "FQDN represented by the resource.",
        "in": "query",
        "schema": {
          "type": "string",
          "example": "example.com"
        }
      },
      {
        "name": "filter[port]",
        "description": "Port to use when connecting to the FQDN.",
        "in": "query",
        "schema": {
          "type": "integer",
          "example": 5060
        }
      },
      {
        "name": "filter[dns_record_type]",
        "description": "DNS record type used by the FQDN.",
        "in": "query",
        "schema": {
          "type": "string",
          "example": "a"
        }
      }
    ],
    "responses": {
      "200": {
        "$ref": "../components/responses/ListFqdnsResponse.json"
      },
      "400": {
        "$ref": "../components/responses/BadRequestResponse.json"
      },
      "401": {
        "$ref": "../components/responses/UnauthenticatedResponse.json"
      }
    },
    "x-latency-category": "responsive"
  },
  "post": {
    "description": "Create a new FQDN object.",
    "summary": "Create an FQDN",
    "operationId": "CreateFqdn",
    "tags": [
      "FQDNs"
    ],
    "x-endpoint-cost": "medium",
    "requestBody": {
      "content": {
        "application/json": {
          "schema": {
            "$ref": "../components/schemas/CreateFqdnRequest.json"
          }
        }
      }
    },
    "responses": {
      "201": {
        "$ref": "../components/responses/FqdnResponse.json"
      },
      "401": {
        "$ref": "../components/responses/UnauthenticatedResponse.json"
      },
      "403": {
        "$ref": "../components/responses/UnauthorizedResponse.json"
      },
      "422": {
        "$ref": "../components/responses/UnprocessableEntityResponse.json"
      }
    },
    "x-latency-category": "responsive"
  }
}
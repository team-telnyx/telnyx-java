{
  "title": "Verification",
  "type": "object",
  "properties": {
    "id": {
      "example": "12ade33a-21c0-473b-b055-b3c836e1c292",
      "format": "uuid",
      "type": "string"
    },
    "type": {
      "$ref": "./VerificationType.json"
    },
    "record_type": {
      "$ref": "./VerificationRecordType.json"
    },
    "phone_number": {
      "example": "+13035551234",
      "type": "string",
      "description": "+E164 formatted phone number."
    },
    "verify_profile_id": {
      "example": "12ade33a-21c0-473b-b055-b3c836e1c292",
      "type": "string",
      "format": "uuid",
      "description": "The identifier of the associated Verify profile."
    },
    "custom_code": {
      "example": "43612",
      "type": "string",
